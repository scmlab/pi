ms : mu($0)(un(?&{ADD -> ?(Int,Int).!Int.0;
           NEG -> ?Int.!Int.0} . $0))

mathSrv =
  *((nu c : &{ADD -> ?(Int,Int).!Int.0;
             NEG -> ?Int.!Int.0}) ~ms[~c] .
     c>>{ ADD -> (c(x,y) . c[x+y] . end) ;
          NEG -> (c(x)   . c[0-x] . end)
        })

usr0 = ms(c) . c<<ADD . c[3,4] . c(r) . end
usr1 = ms(c) . c<<NEG . c[5] . c(r) . end

main = mathSrv | usr0 | usr1




